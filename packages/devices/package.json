{
  "name": "@couch/devices",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "bun run build:js && bun run build:types",
    "build:js": "bun build src/index.ts src/devices/android-tv/selectors.ts src/devices/lg-webos/selectors.ts src/devices/philips-android-tv/selectors.ts src/devices/samsung-tizen/selectors.ts --outdir dist --root src --target bun --format esm",
    "build:types": "tsc -b tsconfig.json --force"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "bun": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./android-tv/selectors": {
      "types": "./dist/devices/android-tv/selectors.d.ts",
      "bun": "./src/devices/android-tv/selectors.ts",
      "default": "./dist/devices/android-tv/selectors.js"
    },
    "./lg-webos/selectors": {
      "types": "./dist/devices/lg-webos/selectors.d.ts",
      "bun": "./src/devices/lg-webos/selectors.ts",
      "default": "./dist/devices/lg-webos/selectors.js"
    },
    "./philips-android-tv/selectors": {
      "types": "./dist/devices/philips-android-tv/selectors.d.ts",
      "bun": "./src/devices/philips-android-tv/selectors.ts",
      "default": "./dist/devices/philips-android-tv/selectors.js"
    },
    "./samsung-tizen/selectors": {
      "types": "./dist/devices/samsung-tizen/selectors.d.ts",
      "bun": "./src/devices/samsung-tizen/selectors.ts",
      "default": "./dist/devices/samsung-tizen/selectors.js"
    }
  },
  "dependencies": {
    "@statelyai/inspect": "^0.4.0",
    "valibot": "^1.2.0",
    "xstate": "^5.25.1"
  }
}
